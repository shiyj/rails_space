<% if @users and not @users.empty? %>
<table class="users" border="0" cellpadding="5" cellspacing="1">
	<tr class="header">
		<th>Name</th><th>Age</th><th>Gender</th>
	</tr>
	<% @users.each do |user| %>
	<tr class="<%=cycle('odd','event')%>">
		<td><%= link_to user.name,profile_for(user) %>
		</td>
		<td><%= user.spec.age %></td>
		<td><%= user.spec.gender %></td>
	</tr>
	<% end %>
	<!--<tr>
	<td><%= page_entries_info @specs %></td>
	</tr>-->
	<tr>
		<td colspan = "4" align="right">
		<%= will_paginate @specs %>
		</td>
	</tr>
</table>
<% end %>
